<html t:type="layout" title="Crypto2 SubBytesPG"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
      
      
	<div id="section">			
	<h2>${message:greeting}</h2>
		<t:zone t:id="viewZone"/>
		<p>		
